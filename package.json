{
  "name": "client",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p check:type \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "check": "run-p check:type check:cspell",
    "check:type": "vue-tsc --build",
    "check:cspell": "cspell lint .  --no-progress",
    "prepare": "husky",
    "commit": "git-cz",
    "openapi": "openapi-typescript",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^4.1.0",
    "@unhead/vue": "^2.0.10",
    "@vueuse/core": "^13.4.0",
    "@vueuse/integrations": "^13.4.0",
    "dayjs": "^1.11.13",
    "defu": "^6.1.4",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.42.0",
    "nprogress": "^0.2.0",
    "openapi-fetch": "^0.14.0",
    "pinia": "^3.0.3",
    "theme-colors": "^0.1.0",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.7",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.29.0",
    "@iconify-json/ant-design": "^1.2.5",
    "@iconify-json/line-md": "^1.2.8",
    "@iconify-json/lucide": "^1.2.54",
    "@intlify/unplugin-vue-i18n": "^6.0.8",
    "@tsconfig/node22": "^22.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.14.0",
    "@types/nprogress": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@unocss/eslint-config": "^66.2.3",
    "@unocss/eslint-plugin": "^66.2.3",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vitest/eslint-plugin": "^1.1.39",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cspell": "^9.0.2",
    "cz-git": "^1.11.1",
    "eslint": "^9.22.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-import-x": "^4.15.2",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "~10.0.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "lint-staged": "^16.1.2",
    "npm-run-all2": "^7.0.2",
    "openapi-typescript": "^7.8.0",
    "prettier": "3.5.3",
    "type-fest": "^4.41.0",
    "typescript": "~5.8.0",
    "unocss": "^66.2.3",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.7.0",
    "unplugin-vue-router": "^0.12.0",
    "vite": "^6.2.4",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-mkcert": "^1.17.8",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vite-plugin-vue-layouts-next": "0.1.5",
    "vitest": "^3.1.1",
    "vue-eslint-parser": "^10.1.3",
    "vue-tsc": "^2.2.8"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "lint-staged": {
    "*": "cspell lint . --no-progress",
    "*.md": [
      "prettier --ignore-unknown --write"
    ],
    "*.vue": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{js,jsx,ts,tsx}": [
      "prettier --ignore-unknown  --write",
      "eslint --fix"
    ],
    "*.{scss,less,html,vue,css}": [
      "prettier --ignore-unknown --write"
    ],
    "package.json": [
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write --parser json"
    ]
  }
}
