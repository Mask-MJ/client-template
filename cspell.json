{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "language": "en,en-US",
  "allowCompoundWords": true,
  "words": [
    "commitlintrc",
    "defu",
    "iconify",
    "intlify",
    "lucide",
    "noopener",
    "noreferrer",
    "qrcode",
    "sider",
    "unhead",
    "unocss",
    "unplugin",
    "VITE",
    "vuejs",
    "vueuse"
  ],
  "ignorePaths": [
    "**/node_modules/**",
    "**/dist/**",
    "**/*-dist/**",
    "**/icons/**",
    "pnpm-lock.yaml",
    "**/*.log",
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/__tests__/**"
  ]
}
